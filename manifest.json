
{
  "manifest_version": 3,
  "name": "Test",
  "version": "1.0.0",
  "description": "Reverse text.",
  "icons": {
  },
  "action": {
    "default_popup": "frontend/popup/popup.html",
    "default_icon": {
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "frontend/options/options.html",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["frontend/content/content.js"],
      "css": ["frontend/content/content.css"]
    }
  ],
  "host_permissions": [
    "http://localhost:5000/*"
  ]
}

